CastorMouseMove0Bust : nonoverlap
{
	// えろすカウンタ
	cntEros = cntEros + 5
	_talk_array = IARRAY

	// 好感度高
	if flgLove == 1 {
		// 絶頂
		if cntEros > 70
		{
			eventCastorOrgBust
		}
		// えろす
		elseif cntEros > 40
		{
			_talk_array ,= aryCastorMouseMove0BustLoveErosCommon
			_talk_array ,= aryCastorCommonErosSerif
		}
		// 通常
		else
		{
			_talk_array ,= aryCastorMouseMove0BustLoveNormalCommon
		}
	}
	// 好感度通常
	else
	{
		// 共通
		_talk_array ,= aryCastorMouseMove0BustFriendCommon
	}

	parallel _talk_array
}

