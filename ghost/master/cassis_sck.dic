// ショートカットキー辞書
OnKeyPress
{
	case reference[0] {
		when "t" {
			OnAiTalk
		}
		when "l" {
			if (STRLEN(AYATEMPLATE.LastTalk) > 0) {
				Menu_LASTTALK
			}
		}
		when "m" {
			openMenu
		}
		when "c" {
			Menu_Communicate
		}
		when "d" {
			Menu_Command
		}
		when "a" {
			StartCommunicate
		}
		when "g" {
			if GetShellnameEx == "Gemini" {
				GEMINITALK
			}
		}
	}
}