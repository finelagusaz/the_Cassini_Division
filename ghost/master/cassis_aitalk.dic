//**** 時報/重なり **********************************************************************

//---- OnMinuteChangeイベント -----------------------------------------------------------
OnMinuteChange
{
	//---- 時間の分を見て 0 であれば時報。それ以外のときは重なり処理を行
	if minute == 0
	{
		// 桜吹雪：時間での色替え
		SakurafubukiShiftBase

		//---- 分 = 0 だったので時報
		"\1\s[10]\0\s[0]%hour12時ね。"
		--
		//---- 特定の時間（正午など）ではさらに追加で話す
		if hour == 12
		{
			"\n\n食事は摂らないとダメよ?"
		}
		elseif hour == 15
		{
			"\n\n少しは休憩しないと身体に障るわ。\w9"
		}
		--
		"\e"
	}
	elseif AYATEMPLATE.mikireflag == 1 && flgMikireEnable == "1"
	{
		//---- 分 != 0 、かつ見切れていたら見切れ中のトークを行う
		MikireTalk
	}
}


//**** 見切れ ***************************************************************************

//---- OnSecondChangeイベント -----------------------------------------------------------
OnSecondChange
{
	//---- 1 秒毎に実行される。ここではあまり重い処理を行わないこと
	if AYATEMPLATE.mikireflag == 1 && reference[1] == 0 && flgMikireEnable == "1"
	{
		//---- 見切れの終了
		EndMikireTalk
	}
	AYATEMPLATE.mikireflag = reference[1]
}

//---- 見切れ中 -------------------------------------------------------------------------
MikireTalk
{
	"\1\s[10]\0\s[0]物陰からヒトを見るのはあまり、\w4良い気分ではないわね\w4…\w4…\e"
}
//---- 見切れ終了 -----------------------------------------------------------------------
EndMikireTalk
{
	"\1\s[10]\0\s[6]見るなら正面ね。\s[0]"
}
