RandomTalk
{
    TOSTR(EVAL(CHR(0x22)+RandomTalkEx()+CHR(0x22)))
}

RandomTalkEx : nonoverlap
{
	_talk_array = IARRAY
	_talk_array ,= aryNormalTalk

	_timeslot = GetTimeSlot // 時間帯取得
	_seasonslot = GetSeasonSlot // 季節取得

	//----------------------------------------
	// 季節ごとのトーク
	//----------------------------------------
	case _seasonslot{
	when "春" {
		_talk_array ,= arySpringTalk
	}
	when "夏" {
		_talk_array ,= arySummerTalk
	}
	when "秋" {
		_talk_array ,= aryAutumnTalk
	}
	when "冬" {
		_talk_array ,= aryWinterTalk
		
		if _timeslot == "深夜" {
			_talk_array ,= aryWinterMidnightTalk
		}
	}
	}

	//----------------------------------------
	// シェルごとのトーク
	//----------------------------------------
	case GetShellnameEx {
	when "master" {
		_talk_array ,= aryShellMasterTalk
	}
	when "Eternity Black" {
		_talk_array ,= aryShellEternityBlackTalk
	}
	when "Alternative - Cassis" {
		_talk_array ,= aryShellAlternativeCassisTalk
	}
	when "月光浴" {
		_talk_array ,= aryShellMoonlightTalk
	}
	when "純白" {
		_talk_array ,= aryShellPurewhiteTalk
	}
	when "Gemini" {
		_talk_array ,= aryShellGeminiTalk
	}
	when "マジカル☆カシス" {
		_talk_array ,= aryShellMagicalCassisTalk
	}
	when "ll29yo" {
		_talk_array ,= aryShellLl29yoTalk
	}
	when "あの青い夏", "あの淡い冬" {
		_talk_array ,= aryThatSeasonTalk
	}
	when "青い天使" {
		_talk_array ,= aryShellBlueAngelTalk
	}
	when "空后 / Himmel Königin" {
		_talk_array ,= aryShellHimmelKoniginTalk
	}
	when "Titania" {
		_talk_array ,= aryShellTitaniaTalk
	}
	when "竜后 / Drachen Königin" {
		_talk_array ,= aryShellDrachenKoniginTalk
	}
	when "スワロウテイル" {
		_talk_array ,= aryShellSwallowTailTalk
	}
	}

	//----------------------------------------
	// フラグ別
	//----------------------------------------
	if flgLove == 1 {
		_talk_array ,= aryLoveTalk
		if GetShellnameEx == "Gemini" {
			_talk_array ,= aryShellGeminiLoveTalk
		}
	} else {
		_talk_array ,= aryFriendTalk
	}

	//----------------------------------------
	// 羽種類別
	//----------------------------------------
	// FIXME: この辺の処理を切り出したい
	_tmp_wing_type = getWingType

	// 鳥の羽ではないトーク
	if _tmp_wing_type !_in_ 'Brid' {
		_talk_array ,= WingNotBirdTalk
	}

	// 機械の羽ではないトーク
	if _tmp_wing_type !_in_ 'Inorganic' {
		_talk_array ,= WingNotInorganicTalk
	}

	// 各種類ごとのトーク
	_tmp_wing_types = SPLIT(_tmp_wing_type, ",")
	for _i=0; _i < TOINT(ARRAYSIZE(_tmp_wing_types)); _i++ {
		// NOTE: 空后と竜后は高速で飛べそうなので除外
		case shellname {
			when '竜后 / Drachen Königin', '空后 / Himmel Königin'
				_talk_array ,= aryWingCommonTalk
		}

		_talk_array ,= EVAL(("aryWing" + _tmp_wing_types[i] + "Talk"))
	}

	parallel _talk_array 
}

