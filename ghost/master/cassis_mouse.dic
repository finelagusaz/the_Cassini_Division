//----------------------------------
// 胸なで
//----------------------------------
MouseMove0Bust : nonoverlap
{
	// えろすカウンタ
	cntEros = cntEros + 5
	_talk_array = IARRAY

	// 好感度高
	if flgLove == 1 {
		// 絶頂
		if cntEros > 70
		{
			eventOrgBust
		}
		// えろす
		elseif cntEros > 40
		{
			if GetShellnameEx == "純白"
			{
				_talk_array ,= aryMouseMouve0BustLoveErosShellPurewhite
			}

			_talk_array ,= aryMouseMouve0BustLoveErosCommon
			_talk_array ,= aryCommonErosSerif
		}
		// 通常
		else
		{
			_talk_array ,= aryMouseMouve0BustLoveNormalCommon
		}
	}
	// 好感度通常
	else
	{
		// 胸の大きなシェル向け
		if GetShellnameEx == "月光浴" || \
		   GetShellnameEx == "純白" || \
		   GetShellnameEx == "瑠璃" || \
		   GetShellnameEx == "桜吹雪" || \
		   GetShellnameEx == "あの青い夏" || \
		   GetShellnameEx == "青い天使"
		{
			_talk_array ,= aryMouseMouve0BustFriendGlamourous
		}

		// 共通
		_talk_array ,= aryMouseMouve0BustFriendCommon
	}

	parallel _talk_array
}

