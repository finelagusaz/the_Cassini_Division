//----------------------------------
// 頭なで
//----------------------------------
MouseMove0Head
{
	// 好感度加算
	cntHead++
	_talk_array = IARRAY

	// シェルごとの反応
	case GetShellnameEx {
		when "銀色"
		{
			_talk_array ,= aryMouseMouve0HeadShellGiniro
		}
	}

	// 好感度50以上
	if cntHead > likelimit
	{
		_talk_array ,= aryMouseMouve0HeadLikeCommon
	}

	// 共通
	_talk_array ,= aryMouseMouve0HeadCommon

	parallel _talk_array
}

