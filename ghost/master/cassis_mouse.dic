Mouse_ClickEx0Lip
{
	_talk_array = IARRAY

	_talk_array ,= aryDoubleClick0LipCommon

	if cntHead > likelimit {
		_talk_array ,= aryDoubleClick0LipLikeCommon

		if flgLove == 1 {
			_talk_array ,= aryDoubleClick0LipLoveCommon
		}
	} else {
		_talk_array ,= aryDoubleClick0LipNormalCommon
	}

	parallel _talk_array
}

