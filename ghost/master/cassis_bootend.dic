//---- OnCloseイベント ------------------------------------------------------------------
OnClose
{
	setShellInit
	--
	"\1\s[10]\0\s[0]"
	--
	if basewarename == "斗月"
	{
		"\1\s[10]\0\s[6]帰るにはまだ早いわ。\w9\e"
		"\1\s[10]\0\s[8]どうやら、\w4お邪魔したようね。\w9\w5\-"
	}
	--
	if ghostexlist == "オフィーリア"
	{
		"今日はこの辺で失礼させてもらうわ。\w9\n/
		\s[6]また、\w4話しましょ。\w9"
	}
	elseif ghostexlist == "Hand"
	{
		"\s[6]…\w4…。\w9"
	}
	elseif ghostexlist == "てでぃめりー"
	{
		"\s[6]ほんと、\w4解放とよく言ったものね。\w9"
	}
	elseif ghostexlist == "瞬子"
	{
		"\1\s[10]\0\s[0]またね、\w4瞬子。\w9\n/
		筆が進むといいわね。"
	}
	else
	{
		_timeslot = GetTimeSlot
		if _timeslot == "朝"
		{
			//---- 朝に終了
			"徹夜明け?\w9\n無理はほどほどになさい。"
			"もう行くの?\w9\n気をつけてね。"
			"\s[6]そう、\w4いってらっしゃい。"
			"いってらっしゃいかしら、\w4それともおやすみなさいかしら。\w9\n/
			また\w4あとで。"
		}
		elseif _timeslot == "昼" || _timeslot == "日中"
		{
			//---- 昼/日中に終了
			"さようなら。"
			"また、\w4あとで。"
			"そう、\w4またね。"
		}
		else
		{
			//---- 夜/深夜に終了
			// Gemini:二人
			if GetShellnameEx == "Gemini"
			{
				"\0\s[0]そう、\w4おやすみなさい。\w9\n\n[half]/
				\1\s[10]良い夜を。"
				"\1\s[10]あら、\w4もう寝るの?\w9\n\n[half]/
				\0\s[0]おやすみなさい、\w4良い夢を。"
			}
			else
			{
				"おやすみなさい。"
				"おやすみなさい、\w4良い夢を。"
				"わかったわ。\w9\n/
				おやすみなさい。\w9\n/
				また、\w4明日。"
				"おやすみなさい。\w9\n/
				また、\w4明日会いましょう。"
			}
		}
	}
	--
	"\w9\w9\w9\-"
}


