//**** 初回起動 *************************************************************************

//---- OnFirstBootイベント --------------------------------------------------------------
OnFirstBoot
{
	//---- 変数の初期化
	username         = "あなた"
	stroke           = 0	// 撫で判定用
	teachusername    = 0	// ユーザー名覚え用
	mikireflag       = 0	// 見切れ処理用
	communicateratio = 0	// コミュニケート開始率[%]
	shellname        = master //シェルの名前
	likelimit        = 50   // 好感度基準値
	flgNoSfcnv       = 1    // サーフィス加算フラグ

	//---- Vanishカウントを見て、真の初回か否かを判定する
	if reference[0] == 0
	{
		//---- ほんとうに初めての起動
		"\1\s[10]\0\s[0]…\w4…\w4此処は何処?\w9\n/
		少なくともヒトはいるようね。\w9\n/
		\s[6]ヒトの声を頼りに来たのだから当たり前ね…\w4…。\n\x/
		\0\s[0]そういえば、\w4名前を言っても、\w4聞いても無かったわね。\w9\n私の名前はカシス、\w4あなたの名前は?\w9\n/
		\![open,inputbox,OnInputUsername,-1]"
	}
	else
	{
		//---- 再インストール後の初回起動
		"\1\s[10]\0\s[0]また、\w4会えたわね。\w9\e"
	}
}

//初回起動イベント、名前を覚える
//名前
//{{CHAIN
//	"\1\s[10]\0\s[0]そういえば、\w4名前を言っても、\w4聞いても無かったわね。\w9\n私の名前はカシス、\w4あなたの名前は?\w9\n/
//	\![open,inputbox,OnInputUsername,-1]\e"
//}}CHAIN

//名前を覚えたときの反応
OnInputUsername
{
	if '' _in_ reference[0]
	{
		if reference[0] != '' {
			username = reference[0]
			--
			"\1\s[10]\0\s[0]%username…\w4…\w4いい名前ね。\w9\e"
		}
		else {
			"\1\s[10]\0\s[6]無理に言わなくてもいいわ。\e"
		}		
	}
}

