//---- 朝/昼/夜などをおおざっぱに調べる関数 -------------------------------------------
GetTimeSlot
{
	if hour >= 4 && hour <= 11
	{
		"朝"
	}
	elseif hour == 12 || hour == 13
	{
		"昼"
	}
	elseif hour >= 14 && hour <= 17
	{
		"日中"
	}
	elseif hour >= 18 && hour <= 21
	{
		"夜"
	}
	else
	{
		"深夜"
	}
}

//---- 春夏秋冬などをおおざっぱに以下略 ------------------------------------------------
GetSeasonSlot
{
	case month {
		when 3-5 {
			"春"
		}

		when 6-8 {
			"夏"
		}

		when 9-11 {
			"秋"
		}

		when 12,1,2 {
			"冬"
		}
	}
}
